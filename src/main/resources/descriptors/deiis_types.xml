<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>deiis_types</name>
  <description>Contains the necessary types to implement an n-gram analysis engine.</description>
  <version>1.0</version>
  <vendor/>
  <imports>
    <import name="org.cleartk.TypeSystem"/>
  </imports>
  <types>
    <typeDescription>
      <name>edu.cmu.deiis.types.Annotation</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>casProcessorId</name>
          <description>The unique identifier of the CAS processor that added this annotation to the CAS</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>confidence</name>
          <description>The confidence level assigned to the Annotation by the CAS processor that created it.</description>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.Token</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
    <features>
        <featureDescription>
          <name>POS</name>
          <description>Part of Speech of the token within the sentence</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>lemma</name>
          <description>Stores the lemma of the token</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>ne</name>
          <description>Stores the named entity label</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.NGram</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>elements</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.deiis.types.Annotation</elementType>
        </featureDescription>
        <featureDescription>
          <name>n</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.Answer</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>isCorrect</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.Question</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.AnswerScore</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>score</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>answer</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.Answer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>edu.cmu.deiis.types.AnswerTokens</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>tokens</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.deiis.types.Token</elementType>
        </featureDescription>
        <featureDescription>
          <name>answer</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.Answer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.QuestionTokens</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>tokens</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.deiis.types.Token</elementType>
        </featureDescription>
        <featureDescription>
          <name>question</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.Question</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>edu.cmu.deiis.types.AnswerNGram</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>unigram</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.NGram</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>bigram</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.NGram</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>trigram</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.NGram</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>answer</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.Answer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>edu.cmu.deiis.types.QuestionNGram</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>unigram</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.NGram</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>bigram</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.NGram</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>trigram</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.NGram</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>question</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.Question</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>edu.cmu.deiis.types.Result</name>
      <description>This annotation stores the final result from the analysis that is realized in the Evalutation Annotator</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>n</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>precision</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>edu.cmu.deiis.types.Sentence</name>
      <description>Describes the sentence found by the Core NLP</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.NamedEntity</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
    <features>
        <featureDescription>
          <name>entityType</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
